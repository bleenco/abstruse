{
  "name": "abstruse",
  "version": "1.6.6",
  "description": "Abstruse CI",
  "bin": {
    "abstruse": "./dist/api/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bleenco/abstruse.git"
  },
  "keywords": [
    "abstruse",
    "ci",
    "continuous integration",
    "test integration",
    "test automation",
    "ci integration"
  ],
  "author": "Bleenco OSS Team <oss@bleenco.com>",
  "bugs": {
    "url": "https://github.com/bleenco/abstruse/issues"
  },
  "homepage": "https://github.com/bleenco/abstruse#readme",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "ng": "ng",
    "build": "webpack --config webpack.api.js",
    "dev": "tsc-watch -p src/tsconfig.api.json --onSuccess \"node ./dist/api/index.js\"",
    "start": "ng serve --open",
    "build:app": "ng build --prod --output-path dist/app",
    "e2e": "ng e2e",
    "test": "node ./tests/run_unit.js",
    "test:karma": "ng test",
    "test:karma:ci": "ng test --watch false",
    "test:e2e": "node ./tests/run_e2e.js",
    "lint": "ng lint",
    "test:protractor": "node ./tests/run_protractor.js",
    "preprotractor:ci": "npm run webdriver",
    "protractor:ci": "protractor ./e2e/protractor.conf.ci.js",
    "build-image": "IMAGE_NAME=bleenco/abstruse:${npm_package_version}-dev DOCKER_TAG=${npm_package_version}-dev SOURCE_COMMIT=`git rev-parse HEAD` hooks/build",
    "prebuild:prod": "npm run clean",
    "build:prod": "npm run build && npm run build:app",
    "prepublishOnly": "npm run build:prod",
    "webdriver": "npm run webdriver:update -- --standalone",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver-manager": "webdriver-manager",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@angular/animations": "7.0.1",
    "@angular/common": "7.0.1",
    "@angular/compiler": "7.0.1",
    "@angular/core": "7.0.1",
    "@angular/forms": "7.0.1",
    "@angular/http": "7.0.1",
    "@angular/platform-browser": "7.0.1",
    "@angular/platform-browser-dynamic": "7.0.1",
    "@angular/router": "7.0.1",
    "bcrypt": "^3.0.2",
    "core-js": "^2.5.7",
    "dockerode": "^2.5.7",
    "knex": "^0.13.0",
    "rsa-compat-ssl": "^1.0.0",
    "rxjs": "^6.3.3",
    "sqlite3": "^4.0.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.3",
    "@angular/cli": "7.0.3",
    "@angular/compiler-cli": "7.0.1",
    "@angular/language-service": "7.0.1",
    "@auth0/angular-jwt": "^2.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bookshelf": "^0.9.9",
    "@types/d3": "^5.0.0",
    "@types/express": "^4.16.0",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/mocha": "^5.2.5",
    "@types/multer": "^1.3.7",
    "@types/node": "10.12.0",
    "@types/request": "^2.48.0",
    "@types/temp": "^0.8.32",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.0",
    "ansi_up": "3.0.0",
    "bookshelf": "^0.13.3",
    "bookshelf-json-columns": "^2.1.1",
    "bulma": "^0.7.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.1",
    "clipboard": "^2.0.1",
    "codelyzer": "^4.5.0",
    "command-exists": "^1.2.8",
    "connect-sqlite3": "^0.9.11",
    "cors": "^2.8.4",
    "d3": "^5.7.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "font-awesome5": "^1.0.5",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.3.0",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "monaco-editor": "^0.14.3",
    "multer": "^1.4.1",
    "ng2-datepicker": "^3.1.1",
    "ngx-uploader": "^7.0.0",
    "node-rsa": "^1.0.1",
    "protractor": "~5.4.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "strip-ansi": "^5.0.0",
    "temp": "^0.8.3",
    "tree-kill": "^1.2.0",
    "ts-loader": "^5.2.2",
    "ts-node": "~7.0.1",
    "tsc-watch": "^1.0.30",
    "tslint": "~5.11.0",
    "typescript": "^3.1.3",
    "webpack-cli": "^3.1.2",
    "ws": "^6.1.0",
    "xterm": "^3.8.0",
    "yamljs": "^0.3.0"
  }
}
