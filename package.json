{
  "name": "abstruse",
  "version": "0.0.5",
  "description": "Abstruse CI",
  "bin": {
    "abstruse": "./dist/api/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prebuild": "npm run clean",
    "compile": "npm run lint && tsc && node ./dist/api/index.js",
    "dev": "nodemon -w src/api -i src/app -e ts --exec \"npm run compile\"",
    "build:prod": "npm run build && npm run build:app:prod",
    "build:app": "webpack --env.dev --progress --hide-modules",
    "build:app:prod": "webpack --env.aot -p --progress --hide-modules",
    "start": "webpack-dev-server --env.dev --env.serve --progress",
    "start:aot": "webpack-dev-server --env.aot --env.dev --env.serve --progress",
    "lint": "tslint ./src/api/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bleenco/abstruse.git"
  },
  "keywords": [
    "abstruse",
    "ci",
    "continuous integration",
    "test integration",
    "test automation",
    "ci integration"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/abstruse/issues"
  },
  "homepage": "https://github.com/bleenco/abstruse#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "bookshelf": "^0.10.3",
    "chalk": "^1.1.3",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "fs-extra": "^3.0.1",
    "jsonwebtoken": "^7.4.0",
    "knex": "^0.13.0",
    "node-pty": "^0.6.4",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.1",
    "shelljs": "^0.7.7",
    "sqlite3": "^3.1.8",
    "ws": "^2.3.1",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "@angular/animations": "4.1.1",
    "@angular/common": "4.1.1",
    "@angular/compiler": "4.1.1",
    "@angular/compiler-cli": "4.1.1",
    "@angular/core": "4.1.1",
    "@angular/forms": "4.1.1",
    "@angular/http": "4.1.1",
    "@angular/platform-browser": "4.1.1",
    "@angular/platform-browser-dynamic": "4.1.1",
    "@angular/platform-server": "4.1.1",
    "@angular/router": "4.1.1",
    "@ngtools/webpack": "^1.3.1",
    "@types/body-parser": "^1.16.3",
    "@types/bookshelf": "^0.9.1",
    "@types/chalk": "^0.4.31",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/knex": "0.0.50",
    "@types/node": "^7.0.18",
    "@types/rimraf": "0.0.28",
    "@types/shelljs": "^0.7.1",
    "@types/sqlite3": "^2.2.33",
    "@types/uuid": "^2.0.29",
    "@types/webpack": "^2.2.15",
    "@types/webpack-dev-server": "^1.12.6",
    "@types/ws": "0.0.41",
    "@types/yamljs": "^0.2.30",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "angular2-jwt": "^0.2.3",
    "chokidar": "^1.6.1",
    "codelyzer": "3.0.1",
