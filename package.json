{
  "name": "abstruse",
  "version": "1.0.0-rc.1",
  "description": "Abstruse CI",
  "bin": {
    "abstruse": "./dist/api/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --config webpack.api.js --hide-modules",
    "build:watch": "tsc -w",
    "prebuild": "npm run clean",
    "dev": "tsc-watch -p tsconfig.json --onSuccess \"node ./dist/api/index.js\"",
    "build:prod": "npm run build && npm run build:app:prod",
    "build:app": "webpack --env.dev --progress --hide-modules",
    "build:app:prod": "webpack --env.aot --env.prod -p --hide-modules",
    "start": "webpack-dev-server --env.dev --env.serve --progress --hot",
    "start:aot": "webpack-dev-server --env.aot --env.dev --env.serve --progress --hot",
    "lint": "tslint ./src/**/*.ts",
    "test": "node ./tests/run_unit.js",
    "test:karma": "karma start ./karma.conf.js",
    "test:e2e": "node ./tests/run_e2e.js",
    "test:protractor": "node ./tests/run_protractor.js",
    "preprotractor": "npm run webdriver",
    "protractor:ci": "protractor ./protractor.conf.js --baseUrl http://localhost:6500",
    "preprotractor:ci": "npm run webdriver",
    "webdriver": "npm run webdriver:update -- --standalone",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "protractor:live": "npm run e2e -- --elementExplorer",
    "protractor": "protractor ./protractor.conf.js",
    "protractor:delay": "sleep 30 && protractor ./protractor.conf.js",
    "protractor:abstruse": "node ./tests/run_protractor.js",
    "prepublishOnly": "npm run build:prod",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bleenco/abstruse.git"
  },
  "keywords": [
    "abstruse",
    "ci",
    "continuous integration",
    "test integration",
    "test automation",
    "ci integration"
  ],
  "author": "Bleenco OSS Team <oss@bleenco.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/abstruse/issues"
  },
  "homepage": "https://github.com/bleenco/abstruse#readme",
