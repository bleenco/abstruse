{
  "name": "abstruse",
  "version": "0.2.4",
  "description": "Abstruse CI",
  "bin": {
    "abstruse": "./dist/api/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prebuild": "npm run clean",
    "compile": "tsc && node ./dist/api/index.js",
    "dev": "nodemon -w src/api -i src/app -e ts --exec \"npm run compile\"",
    "build:prod": "npm run build && npm run build:app:prod",
    "build:app": "webpack --env.dev --progress --hide-modules",
    "build:app:prod": "webpack --env.aot -p --progress --hide-modules",
    "start": "webpack-dev-server --env.dev --env.serve --progress",
    "start:aot": "webpack-dev-server --env.aot --env.dev --env.serve --progress",
    "lint": "tslint ./src/api/**/*.ts",
    "test:e2e": "node ./tests/run_e2e.js",
    "test:protractor": "node ./tests/run_protractor.js",
    "preprotractor": "npm run webdriver",
    "protractor:ci": "protractor ./protractor.conf.js --baseUrl http://localhost:6500",
    "preprotractor:ci": "npm run webdriver",
    "webdriver": "npm run webdriver:update -- --standalone",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "protractor:live": "npm run e2e -- --elementExplorer",
    "protractor": "protractor ./protractor.conf.js",
    "protractor:delay": "sleep 30 && protractor ./protractor.conf.js",
    "protractor:travis": "node ./tests/run_protractor.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bleenco/abstruse.git"
  },
  "keywords": [
    "abstruse",
    "ci",
    "continuous integration",
    "test integration",
    "test automation",
    "ci integration"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/abstruse/issues"
  },
  "homepage": "https://github.com/bleenco/abstruse#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.4",
    "chalk": "^2.0.1",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "fs-extra": "^4.0.0",
    "jsonwebtoken": "^7.4.1",
    "knex": "^0.13.0",
    "node-pty": "^0.6.10",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "shelljs": "^0.7.8",
    "sqlite3": "^3.1.8",
    "ws": "^3.0.0",
    "xterm": "^2.8.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@angular/animations": "4.3.1",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/compiler-cli": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/platform-server": "4.3.1",
    "@angular/router": "4.3.1",
    "@ngtools/webpack": "^1.5.1",
    "@types/body-parser": "^1.16.4",
    "@types/bookshelf": "^0.9.3",
