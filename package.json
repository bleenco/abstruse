    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "webpack": "^2.5.0",
